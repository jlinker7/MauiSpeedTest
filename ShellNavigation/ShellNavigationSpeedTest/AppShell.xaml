<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ShellNavigationSpeedTest.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ShellNavigationSpeedTest"
    Shell.FlyoutBehavior="Disabled"
    Title="ShellNavigationSpeedTest">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
    Title="PinPage"
    ContentTemplate="{DataTemplate local:PinPage}"
    Route="PinPage" />

    <ShellContent
    Title="Flat Pin Page"
    ContentTemplate="{DataTemplate local:FlatPinPage}"
    Route="FlatPinPage" />

</Shell>
